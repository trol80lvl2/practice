<div class="background back2">
    <h1 class="h2">@ViewBag.Back.NameCat</h1>
</div>
<div class="row">
    <div class="col-md-2">

    </div>
    <div class="col-md-9">
        <div class="inline">
            @foreach (var item in ViewBag.List)
            {
                    <div class="cat-item">
                        <img class="cat-pic" src="/img/@item.Path" style="width:100%">
                        <h6 style="margin-top:5px;" class="card-title btn-mid">@item.Manufacturer-@item.Model</h6>
                    </div>
            }
        </div>
    </div>
    <div class="col-md-1">
    </div>
</div>