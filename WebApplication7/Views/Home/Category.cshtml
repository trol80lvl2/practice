<div class="background back2">
    <h1 class="h2">ТЕХНІКА В ПРОКАТ</h1>
</div>
<div class="row">
    <div class="col-md-2">

    </div>
    <div class="col-md-9">
        <div class="inline">
            @foreach (var item in ViewBag.Category)
            {
            <a href="/home/list?id=@item.IdCat">
                <div class="cat-item">
                    <img class="cat-pic" src="~/img/@item.Path" style="width:100%">
                    <h6 style="margin-top:5px;" class="card-title btn-mid">@item.NameCat</h6>
                </div>
            </a>

            }
            </div>
    </div>
    <div class="col-md-1">
    </div>
</div>